<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="250.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mdigius.uno.Controllers.LoginController">
    <children>
        <ImageView fitHeight="96.0" fitWidth="96.0" layoutX="151.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@imgs/icons/UnoIcon.jpg" />
            </image>
        </ImageView>
        <Label layoutX="154.0" layoutY="115.0" text="Select user!">
            <font>
                <Font name="Arial Bold" size="16.0" />
            </font>
        </Label>
        <Button fx:id="cancelBtn" layoutX="220.0" layoutY="212.0" mnemonicParsing="false" onAction="#onCancel" prefWidth="70.0" text="Cancel" />
        <Button layoutX="317.0" layoutY="212.0" mnemonicParsing="false" onAction="#onLogin" prefHeight="26.0" prefWidth="70.0" text="Login" />
      <ComboBox fx:id="userCombo" layoutX="125.0" layoutY="153.0" onAction="#onCombo" prefWidth="150.0" />
    </children>
</AnchorPane>
